@model BookShop.Areas.Admin.ViewModels.CategoryListBoxViewModel

<div class="form-group">
    @Html.LabelFor(model => model.categoryName, htmlAttributes: new { @class = "control-label" })
    <div class="col-md-10">
        @Html.ListBoxFor(model => model.selectedcategory, Model.categories, htmlAttributes: new { @class = "form-control selectpicker", data_live_search = "true" })
    </div>
</div>
